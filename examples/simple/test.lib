/* Minimal Liberty file for testing */
library (test) {
  comment : "Test library for TritonPart";
  delay_model : table_lookup;
  
  time_unit : "1ns";
  voltage_unit : "1V";
  current_unit : "1mA";
  capacitive_load_unit (1,pf);
  pulling_resistance_unit : "1kohm";
  
  default_operating_conditions : typical;
  
  operating_conditions (typical) {
    temperature : 25;
    voltage : 1.0;
  }
  
  /* Simple buffer cell */
  cell (BUF_X1) {
    area : 1.0;
    pin (A) {
      direction : input;
      capacitance : 0.001;
    }
    pin (Y) {
      direction : output;
      function : "A";
      timing() {
        related_pin : "A";
        intrinsic_rise : 0.1;
        intrinsic_fall : 0.1;
        rise_resistance : 0.1;
        fall_resistance : 0.1;
      }
    }
  }
  
  /* Simple DFF cell */
  cell (DFF_X1) {
    area : 4.0;
    ff (IQ, IQN) {
      clocked_on : "CK";
      next_state : "D";
    }
    pin (D) {
      direction : input;
      capacitance : 0.001;
    }
    pin (CK) {
      direction : input;
      capacitance : 0.002;
      clock : true;
    }
    pin (Q) {
      direction : output;
      function : "IQ";
    }
  }
}
